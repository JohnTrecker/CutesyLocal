{
  "name": "cutesylocal-server",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "6.2.2",
    "npm": "3.10.9"
  },
  "now": {
    "engines": {
      "node": "6.x.x"
    },
    "alias": "cutesylocal.com"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "dependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-0": "6.5.0",
    "body-parser": "^1.16.0",
    "dotenv": "^4.0.0",
    "express": "4.13.3",
    "fs": "0.0.2",
    "mongoose": "^4.7.1",
    "morgan": "^1.7.0",
    "underscore": "^1.8.3"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "now-start": "NODE_ENV=production node server/server.js",
    "server": "babel-node server/server.js",
    "client": "babel-node start-client.js",
    "deploy": "now -e DB_USER=@dbuser -e DB_PASSWORD=@dbpassword -e DB_URL=@dburl -e MAPBOXGL_ACCESS_TOKEN=@mbgltoken -e FACEBOOK_CLIENT_ID=@fbid"
  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "connect-flash": "^0.1.1",
    "dotenv": "^4.0.0"
  }
